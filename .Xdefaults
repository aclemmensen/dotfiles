!General
!urxvt*termName:  rxvt-256color
urxvt*loginShell:         true
urxvt*scrollBar:         false
urxvt*secondaryScroll:    true
urxvt*saveLines:         65535
urxvt*cursorBlink:       false
urxvt*urgentOnBell:       true
!urxvt*override-redirect:false
!urxvt*borderLess:       false
!urxvt*internalBorder:       0
!urxvt*externalBorder:       0

! Extensions
!urxvt*perl-lib:        /usr/lib/urxvt/perl/
!urxvt*perl-ext-common: default,matcher,searchable-scrollback
!urxvt*urlLauncher:     /usr/bin/firefox
!urxvt*matcher.button:  1
!  - catch ugly URLs

! Appearance
!  - use a bitmap font
!urxvt*font:        -xos4-terminus-medium-*-*-*-12-*-*-*-*-*-*-*
!urxvt*boldFont:    -xos4-terminus-bold-*-*-*-12-*-*-*-*-*-*-*
!  - use xft for drawing fonts
!urxvt*font:       xft:Terminus
URxvt.font:		xft:Fira Mono Medium for Powerline:pixelsize=12
!  - cursor
urxvt*cursorColor: #DCDCCC
!  - pseudo transparency
!urxvt*shading:    50
!urxvt*transparent:true
!
!  - color scheme
! tangoesque scheme
!*background: #111111
!*foreground: #babdb6
!! Black (not tango) + DarkGrey
!*color0:  #000000
!*color8:  #555753
!! DarkRed + Red
!*color1:  #ff6565
!*color9:  #ff8d8d
!! DarkGreen + Green
!*color2:  #93d44f
!*color10: #c8e7a8
!! DarkYellow + Yellow
!*color3:  #eab93d
!*color11: #ffc123
!! DarkBlue + Blue
!*color4:  #204a87
!*color12: #3465a4
!! DarkMagenta + Magenta
!*color5:  #ce5c00
!*color13: #f57900
!!DarkCyan + Cyan (both not tango)
!*color6:  #89b6e2
!*color14: #46a4ff
!! LightGrey + White
!*color7:  #cccccc
!*color15: #ffffff

Xfg.dpi: 96
Xft.antialias: true
Xfg.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight

XTerm*faceName:		Terminus
XTerm*faceSize:		10
xterm*saveLines:	512

URxvt.background:	#1E2426
URxvt.foreground:	#D3D7CF

! Black:
URxvt.color0:	#000000
URxvt.color8:	#555753
! Red:
URxvt.color1:	#8B2929
URxvt.color9:	#EF2929
! Green:
URxvt.color2:	#52A302
URxvt.color10:	#8ae234
! Yellow:
URxvt.color3:	#FCAF3E
URxvt.color11:	#FFE13E
! Blue:
URxvt.color4:	#3465A4
URxvt.color12:	#729FCF
! Magenta:
URxvt.color5:	#AA00AA
URxvt.color13:	#8B008B
! Cyan:
URxvt.color6:	#008B8B
URxvt.color14:	#00FFFF
! White:
URxvt.color7:	#BABDB6
URxvt.color15:	#EEEEEC
